<script type="text/javascript">

var aqiData = [
  ["北京", 90],
  ["上海", 50],
  ["福州", 10],
  ["广州", 50],
  ["成都", 90],
  ["西安", 100]
];

(function () {
  var a = document.getElementById('aqi-list');
  var b = ["一","二","三","四","五"];
  var c = "";
  aqiData.filter(function(ele){return ele[1]>60;}).sort(function(a,b){return b[1]-a[1];}).forEach(function(ele,index){
  	  c+="<li>第"+b[index]+ele[0]+ele[1]+"</li>"
  })
      a.innerHTML = c
 

})();

</script>
